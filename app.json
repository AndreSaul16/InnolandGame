{
  "expo": {
    "name": "InnolandGame",
    "slug": "InnolandGame",
    "version": "1.0.0-beta",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.android.InnolandGame",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO"
      ],
      "package": "com.android.InnolandGame"
    },
    "plugins": [
      [
        "expo-camera",
        {
          "cameraPermission": "Necesitamos acceso a tu cámara para escanear los códigos QR de INNOLAND."
        }
      ],
      "expo-font"
    ],
    "extra": {
      "eas": {
        "projectId": "9e7f0195-cd2c-46ba-85b1-73399b8744a9"
      }
    }
  }
}
